<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- textviewer -->
	<defaultcontrol>61</defaultcontrol>

	<controls>

		<include>GlobalOverlay</include>

		<control type="group">
			<posx>240</posx>
			<posy>83</posy>

			<control type="image">
				<posx>-30</posx>
				<posy>-30</posy>
				<width>1500</width>
				<height>975</height>
				<texture border="3">global/Panel.png</texture>
			</control>

			<control type="label" id="1">
				<!-- header label -->
				<posx>15</posx>
				<posy>48</posy>
				<width>1410</width>
				<height>45</height>
			</control>

			<control type="textbox" id="5">
				<!-- textarea -->
				<posx>15</posx>
				<posy>108</posy>
				<width>1383</width>
				<height>780</height>
				<align>justify</align>
			</control>

			<control type="scrollbar" id="61">
				<posx>1410</posx>
				<posy>108</posy>
				<width>38</width>
				<height>780</height>
				<onleft>Close</onleft>
				<onright>Close</onright>
				<ondown>61</ondown>
				<onup>61</onup>
			</control>

		</control>

	</controls>

</window>
