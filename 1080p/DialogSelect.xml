<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- selectdialog -->
	<defaultcontrol always="true">3</defaultcontrol>
	<allowoverlay>yes</allowoverlay>

	<controls>

		<include>GlobalOverlay</include>

		<control type="group">
			<posx>450</posx>
			<posy>120</posy>

			<control type="image">
				<posx>-30</posx>
				<posy>-30</posy>
				<width>1080</width>
				<height>900</height>
				<texture border="3">global/Panel.png</texture>
			</control>

			<control type="label" id="1">
				<!-- header label -->
				<posx>15</posx>
				<posy>15</posy>
				<width>990</width>
				<align>center</align>
				<font>Light38</font>
				<label>13406</label>
			</control>

			<control type="list" id="3">
				<posx>15</posx>
				<posy>120</posy>
				<width>660</width>
				<height>690</height>
				<onleft>5</onleft>
				<onright>5</onright>
				<onup>3</onup>
				<ondown>3</ondown>

				<itemlayout height="69">

					<control type="image">
						<width>660</width>
						<height>66</height>
						<texture border="9">global/ButtonNF.png</texture>
					</control>

					<control type="label">
						<posx>30</posx>
						<width>600</width>
						<info>ListItem.Label</info>
					</control>

					<control type="label">
						<posx>630</posx>
						<width>600</width>
						<align>right</align>
						<info>ListItem.Label2</info>
					</control>

				</itemlayout>

				<focusedlayout height="69">

					<control type="image">
						<width>660</width>
						<height>66</height>
						<texture border="9">global/ButtonFO.png</texture>
						<animation effect="fade" start="100" end="50" condition="!Control.HasFocus(3)">Conditional</animation>
					</control>

					<control type="label">
						<posx>30</posx>
						<width>600</width>
						<info>ListItem.Label</info>
					</control>

					<control type="label">
						<posx>630</posx>
						<width>600</width>
						<align>right</align>
						<info>ListItem.Label2</info>
					</control>

				</focusedlayout>

			</control>

			<control type="list" id="6">
				<posx>15</posx>
				<posy>120</posy>
				<width>660</width>
				<height>690</height>
				<onleft>5</onleft>
				<onright>5</onright>
				<onup>6</onup>
				<ondown>6</ondown>

				<itemlayout height="138">

					<control type="image">
						<width>660</width>
						<height>135</height>
						<texture border="9">global/ButtonNF.png</texture>
					</control>

					<control type="image">
						<posx>12</posx>
						<posy>12</posy>
						<width>111</width>
						<height>111</height>
						<texture fallback="NoArt.png">$INFO[Listitem.Icon]</texture>
					</control>

					<control type="label">
						<posx>150</posx>
						<width>600</width>
						<height>135</height>
						<info>ListItem.Label</info>
					</control>

					<control type="label">
						<posx>630</posx>
						<width>600</width>
						<height>135</height>
						<align>right</align>
						<info>ListItem.Label2</info>
					</control>

				</itemlayout>

				<focusedlayout height="138">

					<control type="image">
						<width>660</width>
						<height>135</height>
						<texture border="9">global/ButtonFO.png</texture>
						<animation effect="fade" start="100" end="50" condition="!Control.HasFocus(6)">Conditional</animation>
					</control>

					<control type="image">
						<posx>12</posx>
						<posy>12</posy>
						<width>111</width>
						<height>111</height>
						<texture fallback="NoArt.png">$INFO[Listitem.Icon]</texture>
					</control>

					<control type="label">
						<posx>150</posx>
						<width>600</width>
						<height>135</height>
						<info>ListItem.Label</info>
					</control>

					<control type="label">
						<posx>630</posx>
						<width>600</width>
						<height>135</height>
						<align>right</align>
						<info>ListItem.Label2</info>
					</control>

				</focusedlayout>

			</control>

			<control type="label" id="2">
				<!-- Number Of Files -->
				<posx>705</posx>
				<posy>120</posy>
				<width>300</width>
				<align>center</align>
				<label>-</label>
			</control>

			<control type="button" id="5">
				<!-- Manual -->
				<posx>705</posx>
				<posy>189</posy>
				<width>300</width>
				<label>368</label>
				<onleft>3</onleft>
				<onright>3</onright>
				<onup>3</onup>
				<ondown>3</ondown>
			</control>

			<control type="image">
				<!-- Large Icon Image -->
				<posx>705</posx>
				<posy>357</posy>
				<width>300</width>
				<height>450</height>
				<texture fallback="NoArt.png">$INFO[Container(6).ListItem.Icon]</texture>
				<aspectratio align="center" aligny="bottom">keep</aspectratio>
				<bordertexture border="9">global/ButtonFO.png</bordertexture>
				<bordersize>8</bordersize>
			</control>

		</control>

	</controls>

</window>