<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- sliderdialog -->
	<defaultcontrol always="true">11</defaultcontrol>
	<allowoverlay>yes</allowoverlay>

	<controls>

		<control type="group">
			<posx>638</posx>
			<posy>0</posy>

			<control type="image">
				<posx>-30</posx>
				<posy>-30</posy>
				<width>690</width>
				<height>162</height>
				<texture border="3">global/Panel.png</texture>
			</control>

			<control type="label" id="10">
				<!-- Heading -->
				<posx>15</posx>
				<width>375</width>
				<label>-</label>
				<textcolor>blue</textcolor>
			</control>

			<control type="label" id="12">
				<!-- Label -->
				<posx>615</posx>
				<width>375</width>
				<label>-</label>
				<align>right</align>
			</control>

			<control type="slider" id="11">
				<!-- Slider -->
				<posx>15</posx>
				<posy>66</posy>
				<width>600</width>
				<onup>-</onup>
				<ondown>-</ondown>
				<onleft>11</onleft>
				<onright>11</onright>
			</control>

		</control>

	</controls>

</window>
