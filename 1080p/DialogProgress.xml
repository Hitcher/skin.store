<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- progressdialog -->
	<defaultcontrol always="true">10</defaultcontrol>
	<allowoverlay>yes</allowoverlay>

	<controls>

		<include>GlobalOverlay</include>

		<control type="group">
			<posx>570</posx>
			<posy>333</posy>

			<control type="image">
				<posx>-30</posx>
				<posy>-30</posy>
				<width>840</width>
				<height>414</height>
				<texture border="3">global/Panel.png</texture>
			</control>

			<control type="label" id="1">
				<!-- heading -->
				<posx>15</posx>
				<width>750</width>
				<label>-</label>
				<font>Light38</font>
								<align>center</align>
			</control>

			<control type="label" id="2">
				<!-- Line 1 -->
				<posx>15</posx>
				<posy>75</posy>
				<width>750</width>
				<label>-</label>
			</control>

			<control type="label" id="3">
				<!-- Line 2 -->
				<posx>15</posx>
				<posy>113</posy>
				<width>750</width>
				<label>-</label>
			</control>

			<control type="label" id="4">
				<!-- Line 3 -->
				<posx>15</posx>
				<posy>150</posy>
				<width>750</width>
				<label>-</label>
			</control>

			<control type="progress">
				<!-- Progressbar -->
				<posx>15</posx>
				<posy>225</posy>
				<width>750</width>
				<height>27</height>
				<info>System.Progressbar</info>
			</control>

			<control type="button" id="10">
				<!-- Cancel -->
				<posx>278</posx>
				<posy>267</posy>
				<width>225</width>
				<label>222</label>
				<onleft>10</onleft>
				<onright>10</onright>
				<onup>10</onup>
				<ondown>10</ondown>
			</control>

		</control>

	</controls>

</window>