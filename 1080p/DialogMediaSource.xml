				<focusedlayout height="69">

					<control type="image">
						<width>660</width>
						<height>66</height>
						<texture border="9">global/ButtonFO.png</texture>
						<animation effect="fade" start="100" end="50" condition="!Control.HasFocus(10)">Conditional</animation>
					</control>

					<control type="label">
						<posx>30</posx>
						<width>600</width>
