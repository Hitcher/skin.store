<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- smartplaylistrule -->
	<defaultcontrol always="true">9001</defaultcontrol>
	<allowoverlay>no</allowoverlay>

	<controls>

		<include>GlobalOverlay</include>

		<control type="group">
			<posx>360</posx>
			<posy>330</posy>

			<control type="image">
				<!-- background image -->
				<posx>-30</posx>
				<posy>-30</posy>
				<width>1260</width>
				<height>480</height>
				<texture border="40">ContentPanel.png</texture>
			</control>

			<control type="label">
				<!-- header label -->
				<posx>30</posx>
				<posy>27</posy>
				<width>1140</width>
				<font>Light38</font>
				<align>center</align>
				<label>21421</label>
			</control>

			<control type="label">
				<!-- Rule match label -->
				<posx>60</posx>
				<posy>75</posy>
				<width>1080</width>
				<align>center</align>
				<textcolor>blue</textcolor>
				<label>21422</label>
			</control>

			<control type="group" id="9001">

				<control type="spincontrolex" id="15">
					<!-- Rule Field -->
					<posx>15</posx>
					<posy>150</posy>
					<width>563</width>
					<font>-</font>
					<onright>16</onright>
					<onleft>16</onleft>
					<onup>9000</onup>
					<ondown>17</ondown>
				</control>

				<control type="label">
					<!-- Rule Field label -->
					<posx>30</posx>
					<posy>150</posy>
					<width>450</width>
					<label>$INFO[Control.GetLabel(15)]</label>
				</control>

				<control type="spincontrolex" id="16">
					<!-- Rule operator -->
					<posx>623</posx>
					<posy>150</posy>
					<width>563</width>
					<font>-</font>
					<onright>15</onright>
					<onleft>15</onleft>
					<onup>9000</onup>
					<ondown>17</ondown>
				</control>

				<control type="label">
					<!-- Rule Field label -->
					<posx>638</posx>
					<posy>150</posy>
					<width>300</width>
					<label>$INFO[Control.GetLabel(16)]</label>
				</control>

			</control>

			<control type="button" id="17">
				<!-- Value Button -->
				<posx>30</posx>
				<posy>234</posy>
				<width>878</width>
				<label>-</label>
				<onup>9001</onup>
				<onleft>20</onleft>
				<onright>20</onright>
				<ondown>9000</ondown>
			</control>

			<control type="button" id="20">
				<!-- Browse Button -->
				<posx>915</posx>
				<posy>234</posy>
				<width>255</width>
				<label>1024</label>
				<onup>9001</onup>
				<onleft>17</onleft>
				<onright>17</onright>
				<ondown>9000</ondown>
			</control>

			<control type="group" id="9000">

				<control type="button" id="18">
					<!-- Ok Button -->
					<posx>278</posx>
					<posy>338</posy>
					<width>300</width>
					<label>186</label>
					<onup>17</onup>
					<onleft>19</onleft>
					<onright>19</onright>
					<ondown>9001</ondown>
				</control>

				<control type="button" id="19">
					<!-- Cancel Button -->
					<posx>623</posx>
					<posy>338</posy>
					<width>300</width>
					<label>222</label>
					<onup>17</onup>
					<onleft>18</onleft>
					<onright>19</onright>
					<ondown>9001</ondown>
				</control>

			</control>

		</control>

	</controls>

</window>